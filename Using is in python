programa=input("Digite alguma coisa: ")
print(f"O tipo primitivo é ,{type(programa)}")
print(f"Só tem espaços?,{programa.isspace()}")
print(f"Só tem letra?, {programa.isalpha()}")
print(f"Só tem numero?, {programa.isalnum()}")
print(f"É alfanumerico?, {programa.isalnum()}")
print(f"É um numero?, {programa.isnumeric()}")
print(f"É decimal?,{programa.isdecimal()}")
print(f"Está em maiuscula?, {programa.isupper()}")
print(f"Está em minuscula?, {programa.islower()}")
print(f"Está capitalizada?, {programa.istitle()}")
